includes:
    - vendor/nunomaduro/larastan/extension.neon
    - vendor/tomasvotruba/bladestan/config/extension.neon

parameters:
    level: 5
    paths:
    - app
    - bootstrap
    - config
    - database
    - routes
    - public
    - resources
    - routes
    bootstrapFiles:
        - bootstrap/app.php
    scanDirectories:
        - app/Contracts/Interfaces
        - app/Enums
        - app/Support
        - app/Models
    excludePaths:
        - old-system-legado
        - node_modules
        - vendor
